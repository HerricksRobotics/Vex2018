#pragma config(Sensor, in1,    gyro,           sensorGyro)
#pragma config(Sensor, in2,    gyro2,          sensorGyro)
#pragma config(Sensor, dgtl1,  rightEncoder,   sensorQuadEncoder)
#pragma config(Sensor, dgtl3,  leftEncoder,    sensorQuadEncoder)
#pragma config(Sensor, dgtl5,  touchSensor,    sensorTouch)
#pragma config(Motor,  port1,           leftBack,      tmotorVex393HighSpeed_HBridge, openLoop, reversed)
#pragma config(Motor,  port2,           leftMid,       tmotorVex393HighSpeed_MC29, openLoop)
#pragma config(Motor,  port3,           rightFront,    tmotorVex393HighSpeed_MC29, openLoop)
#pragma config(Motor,  port4,           mobileLift,    tmotorVex393_MC29, openLoop)
#pragma config(Motor,  port5,           elevatorLift,  tmotorVex393_MC29, openLoop)
#pragma config(Motor,  port6,           swingArm,      tmotorVex393_MC29, openLoop)
#pragma config(Motor,  port7,           grabber,       tmotorVex393_MC29, openLoop)
#pragma config(Motor,  port8,           rightBack,     tmotorVex393HighSpeed_MC29, openLoop)
#pragma config(Motor,  port9,           rightMid,      tmotorVex393HighSpeed_MC29, openLoop, reversed)
#pragma config(Motor,  port10,          leftFront,     tmotorVex393HighSpeed_HBridge, openLoop, reversed)
//*!!Code automatically generated by 'ROBOTC' configuration wizard               !!*//

void moveForward(int speed) {
	motor[leftBack] = speed;
	motor[leftMid] = speed;
	motor[leftFront] = speed;
	motor[rightBack] = speed;
	motor[rightMid] = speed;
	motor[rightFront] = speed;
}

void turnRight(int speed) {
	motor[leftBack] = speed;
	motor[leftMid] = speed;
	motor[leftFront] = speed;
}

void turnLeft(int speed) {
	motor[rightBack] = speed;
	motor[rightMid] = speed;
	motor[rightFront] = speed;
}

task main()
{
	SensorValue[rightEncoder] = 0;
	SensorType[in1] = sensorNone;
	SensorType[in2] = sensorNone;
	wait1Msec(1000);
	SensorType[in1] = sensorGyro;
	SensorType[in2] = sensorGyro;
	wait1Msec(2000);
	
	//Roll grabber down
	motor[grabber] = 127;
	
	//Raise the elevator lift
	motor[elevatorLift] = -127;
	wait1Msec(1000);
	motor[elevatorLift] = 0;
	
	//Lower the mobile goal lift
	motor[mobileLift] = 127;
	wait1Msec(1000);
	motor[mobileLift] = 0;
	
	//Move Forward
	while (4 * PI * SensorValue[rightEncoder] / 360 < 28) {
		moveForward(127);
	}
	moveForward(0);
	
	//Raise the mobile goal lift
	motor[mobileLift] = -127;
	wait1Msec(1000);
	motor[mobileLift] = 0;
	
	//Lower the elevator lift
	motor[elevatorLift] = 127;
	wait1Msec(1000);
	motor[elevatorLift] = 0;
	
	//Turn the grabber off
	motor[grabber] = 0;
	
	//Move Backward
	while(4 * PI * SensorValue[rightEncoder] / 360 > 0) {
		moveForward(-127);
	}
	moveForward(0);
	
	//Turn Right
	while( abs( (SensorValue[gyro] + SensorValue[gyro2]) / 2 ) < 1350) {
		turnRight(127);
	}
	turnLeft(0);
	
	//Reset the rightEncoder
	SensorValue[rightEncoder] = 0;
	
	//Move Forward
	while (4 * PI * SensorValue[rightEncoder] / 360 < 12) {
		moveForward(127);
	}
	moveForward(0);
	
	//Reset gyro
	SensorValue[gyro] = 0;
	SensorValue[gyro2] = 0;
	
	//Turn Left 90 Degrees
	while ( abs( (SensorValue[gyro] + SensorValue[gyro2]) / 2) < 900) {
		turnLeft(127);
	}
	
	//Raise the elevator lift
	motor[elevatorLift] = -127;
	wait1Msec(1000);
	motor[elevatorLift] = 0;
	
	//Move Forward at full speed
	moveForward(127);
	wait1Msec(2000);
	moveForward(0);
	
	//Lower the mobile goal lift
	motor[mobileLift] = 127;
	wait1Msec(1000);
	motor[mobileLift] = 0;
	
	//Lower Elevator Lift
	motor[elevatorLift] = 127;
	wait1Msec(1000);
	motor[elevatorLift] = 0;
	
	//Move backward at full speed
	moveForward(-127);
	wait1Msec(2000);
	moveForward(0);
	//

	//Raise elevator lift
	motor[elevatorLift] = -127;
	wait1Msec(1000);
	motor[elevatorLift] = 0;

	//Raise mobile goal
	motor[mobileLift] = -127;
	wait1Msec(1000);
	motor[mobileLift] = 0;	
	
	//Lower elevator lift
	motor[elevatorLift] = 127;
	wait1Msec(1000);
	motor[elevatorLift] = 0;
	
	//Reset gyro
	SensorValue[gyro] = 0;
	SensorValue[gyro2] = 0;
	
	//Turn left 45 degrees
	while ( abs( (SensorValue[gyro] + SensorValue[gyro2]) / 2) < 450) {
		turnLeft(127);
	}
	turnLeft(0);
	
	//Reset right encoder value
	SensorValue[rightEncoder] = 0;
	
	//Move forward 36 inches
	while (4 * PI * SensorValue[rightEncoder] / 360 < 36) {
		moveForward(127);
	}
	moveForward(0);
	
	//Reset gyro
	SensorValue[gyro] = 0;
	SensorValue[gyro2] = 0;
	
	//Turn right 90 degrees
	while ( abs( (SensorValue[gyro] + SensorValue[gyro2]) / 2) < 900) {
		turnRight(127);
	}
	turnRight(0);
	
	//Reset right encorder value
	SensorValue[rightEncoder] = 0;
	
	//Move forward 16 inches
	while (4 * PI * SensorValue[rightEncoder] / 360 < 16) {
		moveForward(127);
	}
	moveForward(0);
	
	//Reset gyro
	SensorValue[gyro] = 0;
	SensorValue[gyro2] = 0;
	
	//Turn right 120 degrees
	while ( abs( (SensorValue[gyro] + SensorValue[gyro2]) / 2) < 1200) {
		turnRight(127);
	}
	turnRight(0);	

	//Raise elevator lift
	motor[elevatorLift] = -127;
	wait1Msec(1000);
	motor[elevatorLift] = 0;
	
	//Lower Mobile Goal
	motor[mobileLift] = 127;
	wait1Msec(1000);
	motor[mobileLift] = 0;	
	
	//Reset right encoder
	SensorValue[rightEncoder] = 0;
	
	//Move forward 2 inches
	while (4 * PI * SensorValue[rightEncoder] / 360 < 2) {
		moveForward(127);
	}
	moveForward(0);
	
	//Raise mobile goal
	motor[mobileLift] = -127;
	wait1Msec(1000);
	motor[mobileLift] = 0;
	
	//Lowers elevator lift
	motor[elevatorLift] = 127;
	wait1Msec(1000);
	motor[elevatorLift] = 0;
	
	//Reset gyro
	SensorValue[gyro] = 0;
	SensorValue[gyro2] = 0;
	
	//Turn right 180 degrees
	while ( abs( (SensorValue[gyro] + SensorValue[gyro2]) / 2) < 1800) {
		turnRight(127);
	}
	turnRight(0);	
	
	//Reset right encoder value
	SensorValue[rightEncoder] = 0;
	
	//Move Forward 22 inches
	while (4 * PI * SensorValue[rightEncoder] / 360 < 22) {
		moveForward(127);
	}
	moveForward(0);
	
	//Raise elevator lift
	motor[elevatorLift] = -127;
	wait1Msec(1000);
	motor[elevatorLift] = 0;
	
	//Move forward using time function
	moveForward(127);
	wait1Msec(2500);
	moveForward(0);
	
	//Lower mobile goal
	motor[mobileLift] = 127;
	wait1Msec(1000);
	motor[mobileLift] = 0;
	
	//Move backward with full speed
	moveForward(-127);
	wait1Msec(2500);
	moveForward(0);
	
	//Lower elevator lift
	motor[elevatorLift] = 127;
	wait1Msec(1000);
	motor[elevatorLift] = 0;	
	
	//Reset gyro
	SensorValue[gyro]= 0;
	SensorValue[gyro2]= 0;
	
	//Turn left 190 degrees
	while ( abs( (SensorValue[gyro] + SensorValue[gyro2]) / 2) < 1900) {
		turnLeft(127);
	}
	turnLeft(0);
	
	//Raise elevator lift
	motor[elevatorLift] = -127;
	wait1Msec(1000);
	motor[elevatorLift] = 0;	
	
	//Lower mobile goal
	motor[mobileLift] = -127;
	wait1Msec(1000);
	motor[mobileLift] = 0;
	
	//Reset right encoder values
	SensorValue[rightEncoder] = 0;
	
	//Move forward 9 inches
	while (4 * PI * SensorValue[rightEncoder] / 360 < 9) {
		moveForward(127);
	}
	moveForward(0);
	
	//Raise mobile goal
	motor[mobileLift] = 127;
	wait1Msec(1000);
	motor[mobileLift] = 0;
	
	//Lowers elevator lift
	motor[elevatorLift] = 127;
	wait1Msec(1000);
	motor[elevatorLift] = 0;
	
	//Resets gyro
	SensorValue[gyro]= 0;
	SensorValue[gyro2]= 0;
	
	//Turn 190 degrees right 
	while ( abs( (SensorValue[gyro] + SensorValue[gyro2]) / 2) < 1900) {
		turnRight(127);
	}
	turnRight(0);	
	
	//Reset right encoder value
	SensorValue[rightEncoder] = 0;
	
	//Move forward 22 inches
	while (4 * PI * SensorValue[rightEncoder] / 360 < 22) {
		moveForward(127);
	}
	moveForward(0);	
	
	//Raise the elevator lift
	motor[elevatorLift] = -127;
	wait1Msec(1000);
	motor[elevatorLift] = 0;
	
	//Move forward using time function
	moveForward(127);
	wait1Msec(3000);
	moveForward(0);
	
	//Lowers mobile goal
	motor[mobileLift] = -127;
	wait1Msec(1000);
	motor[mobileLift] = 0; 
	
	//Moves back 
	moveForward(-127);
	wait1Msec(3000);
	moveForward(0);
}

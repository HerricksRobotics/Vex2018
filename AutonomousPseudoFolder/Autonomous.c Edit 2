#pragma config(Motor,  port1,           leftLegs,      tmotorVex393HighSpeed_HBridge, openLoop)
#pragma config(Motor,  port2,           rightLegs,     tmotorVex393HighSpeed_MC29, openLoop)
#pragma config(Motor,  port3,           topLeftArm,    tmotorVex393HighSpeed_MC29, openLoop)
#pragma config(Motor,  port4,           btmLeftArm,    tmotorVex393HighSpeed_MC29, openLoop)
#pragma config(Motor,  port5,           topRightArm,   tmotorVex393HighSpeed_MC29, openLoop)
#pragma config(Motor,  port6,           btmRightArm,   tmotorVex393HighSpeed_MC29, openLoop)
//*!!Code automatically generated by 'ROBOTC' configuration wizard               !!*//

int SPEED = 120;
int TIME = 5; //in seconds

void armForward (int speed){
	motor [topLeftArm] = 127;
	motor [btmLeftArm] = 127;
	motor [topRightArm] = 127;
	motor [btmRightArm] = 127;
}

void armBackward () {
	motor [topLeftArm] = -127;
	motor [btmLeftArm] = -127;
	motor [topRightArm] = -127;
	motor [btmRightArm] = -127;
}

/*void pinchOpen () {
	motor [LeftArmPinch] = -63;
	motor [RightArmPinch] = -63;
}

void pinchClose () {
	motor [LeftArmPinch] = 63;
	motor [RightArmPinch] = 63;
}*/

task main() {
	while(true) {
		motor [leftLegs] = SPEED;
		motor [rightLegs] = SPEED;
		wait1Msec(TIME * 1000);
		motor [leftLegs] = 0;
		motor [rightLegs] = 0;

	//	pinchOpen();
		wait1Msec(Time * 1000);
		armForward(SPEED);
	//	pinchClose();
		armBackward();
		motor [leftLegs] = -SPEED;
		motor [rightLegs] = -SPEED;
		wait1Msec(TIME*1000);
		motor [leftLegs] = 0;
		motor [rightLegs] = 0;
	//	pinchOpen();
	}
}
